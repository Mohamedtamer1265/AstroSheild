{
  "name": "astroshield",
  "version": "1.0.0",
  "description": "Advanced Asteroid Impact Simulation and Planetary Defense System",
  "main": "server/run.py",
  "scripts": {
    "start": "python server/run.py",
    "build": "cd client && npm install && npm run build",
    "install-client": "cd client && npm install",
    "install-server": "cd server && pip install -r requirements.txt",
    "dev": "concurrently \"cd client && npm run dev\" \"cd server && python run.py\"",
    "client": "cd client && npm run dev",
    "server": "cd server && python run.py"
  },
  "keywords": [
    "nasa",
    "asteroid",
    "space",
    "impact",
    "simulation",
    "planetary-defense"
  ],
  "author": "AstroShield Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "python": ">=3.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mohamedtamer1265/AstroSheild.git"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}